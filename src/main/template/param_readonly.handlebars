<div class="param-property hide" data-label="name">{{name}}</div>
<div class="param-property" data-label='{{name}}'>
    <div>
        {{#if isBody}}
            <textarea class='body-textarea ui-form-control' readonly='readonly' name='{{name}}'  data-toggle="tooltip" data-placement="top" title="" data-original-title='{{name}}'>{{default}}</textarea>
            <div class="parameter-content-type"/>
        {{else}}
            {{#if default}}
                {{default}}
            {{/if}}
        {{/if}}
        <div class="small-description">
            {{#if type}}
                <div>Type: <code class="code-signature">{{type}}</code></div>
            {{/if}}
            {{#if in}}
              <div>In <code class="code-signature">{{in}}</code></div>
            {{/if}}
            {{#if description}}
            <div class="markdown">Description: {{description}}</div>
            {{/if}}
        </div>
    </div>
</div>
